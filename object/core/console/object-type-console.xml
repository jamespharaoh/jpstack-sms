<console-module
	name="object-type">

	<console-helper-provider
		object-name="objectType"
		view-priv="root:manage">

		<context-stuff
			name="objectTypeName"
			field="code"/>

		<priv-key
			name="objectType.manage"
			priv="root:manage"/>

	</console-helper-provider>

	<object-context
		name="objectType"
		object-name="objectType">

		<list>

			<object-list-page
				form="list"/>

		</list>

		<object>

			<object-summary-page
				form="summary"/>

			<object-events-page/>

		</object>

	</object-context>

	<forms>

		<form
			name="list"
			object-type="object-type"
			type="read-only">

			<columns>

				<id-field/>
				<code-field/>

			</columns>

		</form>

		<form
			name="summary"
			object-type="object-type"
			type="read-only">

			<columns>

				<id-field/>
				<code-field/>

			</columns>

		</form>

	</forms>

</console-module>
