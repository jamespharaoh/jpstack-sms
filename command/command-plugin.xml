<plugin
	name="command"
	package="wbs.sms.command">

	<models>
		<model name="command"/>
		<model name="commandType"/>
	</models>

	<layer name="entity">
		<bean class="model.CommandTypeBuilder"/>
		<bean class="model.CommandTypeSpec"/>
		<bean class="model.CommandTypesBuilder"/>
		<bean class="model.CommandTypesSpec"/>
	</layer>

	<layer name="logic">
		<bean class="logic.CommandLogicImpl"/>
	</layer>

	<console-modules>
		<console-module name="command"/>
	</console-modules>

</plugin>
