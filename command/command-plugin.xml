<plugin
	name="command"
	package="wbs.sms.command">

	<models>
		<model name="command"/>
		<model name="commandType"/>
	</models>

	<layer name="fixture">
		<bean class="fixture.CommandTypeBuilder"/>
		<bean class="fixture.CommandTypesBuilder"/>
	</layer>

	<layer name="model-meta">
		<bean class="metamodel.CommandTypeSpec"/>
		<bean class="metamodel.CommandTypesSpec"/>
	</layer>

	<layer name="logic">
		<bean class="logic.CommandLogicImpl"/>
	</layer>

	<console-modules>
		<console-module name="command"/>
	</console-modules>

</plugin>
