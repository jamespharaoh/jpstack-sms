<plugin
	name="number-format"
	package="number.format">

	<sql-scripts>
		<sql-data name="number-format-data"/>
	</sql-scripts>

	<models>
		<model name="numberFormat"/>
		<model name="numberFormatPattern"/>
	</models>

	<layer name="logic">
		<bean class="logic.NumberFormatLogicImpl"/>
	</layer>

	<console-modules>
		<console-module name="number-format"/>
		<console-module name="number-format-pattern"/>
	</console-modules>

</plugin>
