<plugin
	name="number-format"
	package="wbs.sms.number.format">

	<plugin-dependencies>
		<plugin-dependency name="sms-core"/>
	</plugin-dependencies>

	<models>
		<model name="numberFormat"/>
		<model name="numberFormatPattern"/>
	</models>

	<layer name="logic">
		<bean class="logic.NumberFormatLogicImplementation"/>
	</layer>

	<fixtures>
		<fixture name="numberFormat"/>
	</fixtures>

	<console-modules>
		<console-module name="number-format"/>
		<console-module name="number-format-pattern"/>
	</console-modules>

</plugin>
