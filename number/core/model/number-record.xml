<record
	name="number"
	type="common">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<code-field
			name="number"/>

		<!-- state -->

		<reference-field
			type="network"/>

		<timestamp-field
			name="archive-date"
			column-type="postgresql"
			nullable="yes"/>

		<yes-no-field
			name="free"
			default="no"/>

	</fields>

</record>