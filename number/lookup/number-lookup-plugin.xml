<plugin
	name="number-lookup"
	package="wbs.sms.number.lookup">

	<models>
		<model name="numberLookup"/>
		<model name="numberLookupType"/>
	</models>

	<layer name="entity">
		<bean class="model.NumberLookupTypeBuilder"/>
		<bean class="model.NumberLookupTypeSpec"/>
		<bean class="model.NumberLookupTypesBuilder"/>
		<bean class="model.NumberLookupTypesSpec"/>
	</layer>

	<layer name="logic">
		<bean class="logic.NumberLookupHelperManager"/>
		<bean class="logic.NumberLookupManagerImpl"/>
	</layer>

</plugin>
