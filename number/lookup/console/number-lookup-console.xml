<console-module
	name="number-lookup">

	<console-helper-provider
		object-name="numberLookup"
		view-priv="parent">

		<context-stuff
			name="numberLookupName"
			field="id"/>

		<priv-key
			name="numberLookup.manage"
			delegate="parent"
			priv="manage"/>

	</console-helper-provider>

	<object-context
		name="numberLookup"
		object-name="numberLookup">

		<object>

			<object-summary-page
				fields="summary"/>

			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="summary"
		object-name="numberLookup">

		<id-field/>
		<parent-field/>
		<code-field/>

	</field-set>

</console-module>
