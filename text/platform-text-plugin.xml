<plugin
	name="platform-text"
	package="wbs.platform.text">

	<sql-scripts>
		<sql-schema name="text"/>
	</sql-scripts>

	<models>
		<model name="text"/>
	</models>

	<layer name="console">
		<component class="console.TextConsoleHooks"/>
		<component class="console.TextCryptor"/>
		<component class="console.TextFormFieldNativeMapping"/>
		<component class="console.TextFormFieldPluginProvider"/>
	</layer>

	<console-modules>
		<console-module name="text"/>
	</console-modules>

</plugin>
