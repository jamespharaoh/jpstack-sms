<record
	name="inbox-multipart-log"
	type="event">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<parent-field
			type="route"/>

		<timestamp-field
			name="timestamp"
			column-type="postgresql"/>

		<!-- data -->

		<string-field
			name="msg-from"/>

		<integer-field
			name="multipart-id"/>

		<integer-field
			name="multipart-seg-max"/>

	</fields>

</record>