<record
	name="delivery"
	type="ephemeral">

	<fields>

		<!-- id -->

		<generated-id-field
			sequence="delivery_notice_queue_id_seq"/>

		<!-- identity -->

		<parent-field
			type="message"/>

		<!-- data -->

		<enum-field
			name="old-message-status"
			type="message-status"/>

		<enum-field
			name="new-message-status"
			type="message-status"/>

	</fields>

</record>