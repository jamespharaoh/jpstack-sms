<plugin
	name="delivery"
	package="wbs.sms.message.delivery">

	<models>
		<model name="delivery"/>
		<model name="deliveryType"/>
	</models>

	<layer name="entity">
		<bean class="model.DeliveryTypeBuilder"/>
		<bean class="model.DeliveryTypeSpec"/>
		<bean class="model.DeliveryTypesBuilder"/>
		<bean class="model.DeliveryTypesSpec"/>
	</layer>

	<layer name="daemon">
		<bean class="daemon.DeliveryDaemon"/>
	</layer>

</plugin>
