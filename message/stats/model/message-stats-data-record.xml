<record
	name="message-stats-data"
	type="composite">

	<fields>

		<integer-field
			name="in-total"
			default="0"/>

		<integer-field
			name="out-total"
			default="0"/>

		<integer-field
			name="out-pending"
			default="0"/>

		<integer-field
			name="out-cancelled"
			default="0"/>

		<integer-field
			name="out-failed"
			default="0"/>

		<integer-field
			name="out-sent"
			default="0"/>

		<integer-field
			name="out-submitted"
			default="0"/>

		<integer-field
			name="out-delivered"
			default="0"/>

		<integer-field
			name="out-undelivered"
			default="0"/>

		<integer-field
			name="out-report-timed-out"
			default="0"/>

		<integer-field
			name="out-held"
			default="0"/>

		<integer-field
			name="out-blacklisted"
			default="0"/>

		<integer-field
			name="out-manually-undelivered"
			default="0"/>

		<integer-field
			name="out-manually-delivered"
			default="0"/>

		<integer-field
			name="out-rejected"
			default="0"/>

	</fields>

</record>
