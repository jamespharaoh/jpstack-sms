<plugin
	name="wap-push"
	package="wbs.sms.message.wap">

	<sql-scripts>
		<sql-data name="wap-push-data"/>
	</sql-scripts>

	<models>
		<model name="wapPushMessage"/>
	</models>

	<layer name="logic">
		<bean class="logic.WapPushLogic"/>
	</layer>

	<layer name="console">
		<bean class="console.WapPushConsolePlugin"/>
		<bean class="console.WapPushMessageSummaryPart"/>
	</layer>

</plugin>
