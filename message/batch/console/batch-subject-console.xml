<console-module
	name="batch-subject">

	<console-helper-provider
		object-name="batchSubject"
		view-delegate-field="parent">

		<context-stuff
			name="batchSubjectName"
			field="id"/>

		<priv-key
			name="batchSubject.manage"
			delegate="parent"
			priv="manage"/>

	</console-helper-provider>

	<object-context
		name="batchSubject"
		object-name="batchSubject">

		<object>

			<object-summary-page
				fields="summary"/>

			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="summary"
		object-name="batchSubject">

		<id-field/>
		<parent-field/>
		<code-field/>

		<object-field
			name="batchSubject"
			finder="batchSubject"/>

	</field-set>

</console-module>
