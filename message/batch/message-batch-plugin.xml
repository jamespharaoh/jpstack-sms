<plugin
	name="message-batch"
	package="wbs.sms.message.batch">

	<sql-scripts>
		<sql-data name="batch-data"/>
	</sql-scripts>

	<models>
		<model name="batch"/>
		<model name="batchSubject"/>
		<model name="batchType"/>
	</models>

	<layer name="fixture">
		<bean class="fixture.BatchTypeBuilder"/>
		<bean class="fixture.BatchTypesBuilder"/>
	</layer>

	<layer name="model-meta">
		<bean class="metamodel.BatchTypeSpec"/>
		<bean class="metamodel.BatchTypesSpec"/>
	</layer>

	<layer name="logic">
		<bean class="logic.BatchLogicImpl"/>
	</layer>

	<console-modules>
		<console-module name="batch"/>
		<console-module name="batch-subject"/>
	</console-modules>

</plugin>
