<console-module
	name="logging">

	<simple-context
		name="logging">

		<priv-key
			name="logging.manage"
			priv="root:manage"/>

		<context-link
			name="staticContext"
			link="loggingStaticContext"
			label="Static contexts"
			priv-key="logging.manage"/>

		<section
			name="rule"
			label="Rules">

			<context-link
				name="global"
				link="loggingGlobalRule"
				label="Global"
				priv-key="logging.manage"/>

			<context-link
				name="static"
				link="loggingStaticContextRule"
				label="Static"
				priv-key="logging.manage"/>

			<context-link
				name="dynamic"
				link="loggingDynamicContextRule"
				label="Dynamic"
				priv-key="logging.manage"/>

		</section>

		<context-link
			name="entry"
			link="loggingEvent"
			label="Entries"
			priv-key="logging.manage"/>

	</simple-context>

</console-module>
