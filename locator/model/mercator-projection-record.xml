<record
	name="mercator-projection"
	type="major">

	<record-interfaces>

		<interface
			package="wbs.sms.locator.model"
			name="MercatorProjection"/>

	</record-interfaces>

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<code-field/>

		<!-- details -->

		<description-field/>

		<!-- data -->

		<reference-field
			type="biaxial-ellipsoid"/>

		<floating-point-field
			name="scale-factor"/>

		<floating-point-field
			name="origin-longitude"/>

		<floating-point-field
			name="origin-latitude"/>

		<floating-point-field
			name="origin-easting"/>

		<floating-point-field
			name="origin-northing"/>

	</fields>

</record>