<plugin
	name="locator"
	package="wbs.sms.locator">

	<models>
		<custom-type name="longLat"/>
		<model name="biaxialEllipsoid"/>
		<model name="locator"/>
		<model name="locatorLog"/>
		<model name="locatorType"/>
		<model name="mercatorProjection"/>
	</models>

	<layer name="model-meta">
		<bean class="metamodel.LongitudeLatitudeFieldSpec"/>
	</layer>

	<layer name="model-generate">
		<bean class="generate.LongitudeLatitudeFieldWriter"/>
	</layer>

	<layer name="entity">
		<bean class="build.LongitudeLatitudeModelFieldBuilder"/>
	</layer>

	<layer name="logic">
		<bean class="logic.LocatorLogicImplementation"/>
		<bean class="logic.LocatorManager"/>
		<bean class="logic.MercatorProjectionConfig"/>
	</layer>

	<console-modules>
		<console-module name="locator"/>
	</console-modules>

</plugin>
