<record
	name="queueType"
	type="type">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<parent-field
			name="parentType"
			type="objectType"
			column="parent_object_type_id"/>

		<code-field/>

		<!-- details -->

		<description-field/>

		<!-- data -->

		<reference-field
			name="subjectType"
			type="objectType"
			column="subject_object_type_id"/>

		<reference-field
			name="refType"
			type="objectType"
			column="ref_object_type_id"/>

		<!-- settings -->

		<integer-field
			name="defaultPriority"
			nullable="yes"/>

	</fields>

</record>