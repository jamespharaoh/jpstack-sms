<console-module
	name="text">

	<console-helper-provider
		object-name="text"
		cryptor="textCryptor">

		<priv-key
			name="text.manage"
			priv="parent:manage"/>

		<context-stuff
			name="textName"
			field="id"/>

	</console-helper-provider>

	<object-context
		name="text"
		object-name="text"
		cryptor="textCryptor">

		<object>

			<object-summary-page
				form="summary"/>

		</object>

	</object-context>

	<forms>

		<form
			name="summary"
			object-type="text"
			type="read-only">

			<columns>

				<id-field/>

			</columns>

		</form>

	</forms>

</console-module>
