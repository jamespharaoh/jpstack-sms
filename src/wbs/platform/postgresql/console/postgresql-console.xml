<console-module
	name="postgresql">

	<simple-context
		name="postgresql">

		<priv-key
			name="postgresql.manage"
			priv="root:manage"/>

		<context-tab-responder-page
			name="activity"/>

		<context-tab-responder-page
			name="tables"/>

		<context-link
			name="maintenance"
			link="postgresqlMaintenance"
			label="Maintenance"
			priv-key="postgresql.manage"/>

	</simple-context>

</console-module>