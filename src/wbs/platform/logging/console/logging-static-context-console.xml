<console-module
	name="logging-static-context">

	<console-helper-provider
		object-name="loggingStaticContext"/>

	<object-context
		name="loggingStaticContext"
		object-name="loggingStaticContext">

		<list>

			<object-list-page
				form="list"/>

		</list>

		<object>

			<object-summary-page
				form="summary"/>

		</object>

	</object-context>

	<forms>

		<form
			name="list"
			object-type="logging-static-context"
			type="read-only">

			<columns>

				<name-field/>

			</columns>

		</form>

		<form
			name="summary"
			object-type="logging-static-context"
			type="read-only">

			<columns>

				<id-field/>
				<code-field/>
				<name-field/>

			</columns>

		</form>

	</forms>

</console-module>