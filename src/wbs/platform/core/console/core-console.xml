<console-module
	name="core">

	<simple-responder
		name="logon"/>

	<simple-responder
		name="home"/>

	<simple-responder
		name="frameSet"/>

	<simple-responder
		name="sidebarMenu"/>

	<simple-responder
		name="redirect"/>

	<simple-file
		path=""
		get-responder="coreFrameSetResponder"
		post-action="coreLogonAction"/>

	<simple-file
		path="/logoff"
		post-action="coreLogoffAction"/>

	<simple-file
		path="/home"
		get-responder="coreHomeResponder"/>

	<simple-file
		path="/sidebar"
		get-responder="coreSidebarMenuResponder"/>

</console-module>
