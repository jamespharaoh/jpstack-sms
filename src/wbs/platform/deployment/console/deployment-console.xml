<console-module
	name="deployment">

	<simple-context
		name="deployment">

		<priv-key
			name="deployment.manage"
			priv="root:manage"/>

		<context-link
			name="version"
			link="deploymentVersion"
			label="Versions"
			priv-key="deployment.manage"/>

		<context-link
			name="api"
			link="apiDeployment"
			label="API"
			priv-key="deployment.manage"/>

		<context-link
			name="console"
			link="consoleDeployment"
			label="Console"
			priv-key="deployment.manage"/>

		<context-link
			name="daemon"
			link="daemonDeployment"
			label="Daemon"
			priv-key="deployment.manage"/>

	</simple-context>

</console-module>
