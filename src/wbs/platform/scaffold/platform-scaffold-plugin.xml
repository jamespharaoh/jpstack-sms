<plugin
	name="platform-scaffold"
	package="wbs.platform.scaffold">

	<models>
		<model name="root"/>
		<model name="slice"/>
	</models>

	<layer name="logic">
		<component class="logic.SliceLogicImplementation"/>
	</layer>

	<fixtures>
		<fixture name="root"/>
		<fixture name="slice"/>
	</fixtures>

	<console-modules>
		<console-module name="root"/>
		<console-module name="slice"/>
	</console-modules>

</plugin>
