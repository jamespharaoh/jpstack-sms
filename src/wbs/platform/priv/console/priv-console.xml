<console-module
	name="priv">

	<console-helper-provider
		object-name="priv"
		view-priv="parent:">

		<context-stuff
			name="privName"
			field="id"/>

		<priv-key
			name="priv.manage"
			priv="parent:manage"/>

	</console-helper-provider>

	<object-context
		name="priv"
		object-name="priv">

		<object>

			<object-summary-page
				form="summary"/>

			<object-events-page/>

		</object>

	</object-context>

	<forms>

		<form
			name="summary"
			object-type="priv"
			type="read-only">

			<columns>

				<id-field/>
				<parent-field/>
				<code-field/>

			</columns>

		</form>

	</forms>

</console-module>
