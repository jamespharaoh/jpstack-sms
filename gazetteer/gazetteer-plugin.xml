<plugin
	name="gazetteer"
	package="gazetteer">

	<dependencies>
		<project name="wbs-sms">
			<plugin name="sms-common"/>
		</project>
	</dependencies>

	<sql-scripts>
		<sql-data name="gazetteer-data"/>
	</sql-scripts>

	<layer name="logic">
		<bean class="logic.GazetteerLogicImpl"/>
	</layer>

	<models>
		<model name="gazetteer"/>
		<model name="gazetteerEntry"/>
	</models>

	<fixtures>
		<fixture name="gazetteer"/>
	</fixtures>

	<console-modules>
		<console-module name="gazetteer"/>
		<console-module name="gazetteer-entry"/>
	</console-modules>

</plugin>
