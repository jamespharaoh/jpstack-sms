<model-meta
	name="gazetteer"
	type="major">

	<fields>

		<!-- id -->

		<generated-id-field/>

		<!-- identity -->

		<parent-field
			type="slice"/>

		<code-field/>

		<!-- details -->

		<name-field/>

		<description-field/>

		<deleted-field/>

	</fields>

	<collections>

		<children-collection
			name="entries"
			type="gazetteerEntry"/>

	</collections>

</model-meta>
