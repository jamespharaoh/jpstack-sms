<plugin
	name="network"
	package="network">

	<dependencies>
		<project name="wbs-sms">
			<plugin name="sms-common"/>
		</project>
	</dependencies>

	<sql-scripts>
		<sql-schema name="network"/>
		<sql-data name="network-data"/>
	</sql-scripts>

	<models>
		<model name="network"/>
		<model name="networkPrefix"/>
	</models>

	<layer name="logic">
		<bean class="logic.NetworkPrefixCache"/>
	</layer>

	<fixtures>
		<fixture name="network"/>
	</fixtures>

	<console-modules>
		<console-module name="network"/>
	</console-modules>

</plugin>
