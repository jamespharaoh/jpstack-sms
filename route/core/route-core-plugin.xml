<plugin
	name="route-core"
	package="route.core">

	<dependencies>
		<project name="wbs-sms">
			<plugin name="sms-common"/>
		</project>
	</dependencies>

	<sql-scripts>
		<sql-schema name="route"/>
		<sql-data name="route-data"/>
	</sql-scripts>

	<models>
		<model name="route"/>
		<type name="routeNetworkBehaviour"/>
	</models>

	<layer name="console">
		<bean class="console.RouteSummaryAdditionalPartManager"/>
		<bean class="console.RouteSummaryAdditionalPart"/>
		<bean class="console.RouteNetworkBehaviourConsoleHelper"/>
	</layer>

	<fixtures>
		<fixture name="route"/>
	</fixtures>

	<console-modules>
		<console-module name="route"/>
	</console-modules>

</plugin>
