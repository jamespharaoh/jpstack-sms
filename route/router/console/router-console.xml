<console-module
	name="router">

	<console-helper-provider
		object-name="router"
		view-delegate-field="parent">

		<context-stuff
			name="routerName"
			field="code"/>

		<priv-key
			name="router.manage"
			delegate="parent"
			priv="manage"/>

	</console-helper-provider>

	<object-context
		name="router"
		object-name="router">

		<object>

			<object-summary-page
				fields="summary"/>

			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="summary"
		object-name="router">

		<id-field/>
		<parent-field/>
		<code-field/>

		<object-field
			name="route"
			finder="route"/>

	</field-set>

</console-module>
