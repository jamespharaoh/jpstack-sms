<console-module
	name="sender">

	<console-helper-provider
		object-name="sender"
		view-priv="parent">

		<priv-key
			name="sender.manage"
			priv="parent:manage"/>

		<context-stuff
			name="senderName"
			field="code"/>

	</console-helper-provider>

	<object-context
		name="sender"
		object-name="sender">

		<list>

			<object-list-page
				fields="list"/>

		</list>

		<object>

			<object-summary-page
				fields="summary"/>

			<object-events-page/>

		</object>

	</object-context>

	<field-set
		name="list"
		object-name="sender">

		<code-field/>
		<description-field/>

	</field-set>

	<field-set
		name="summary"
		object-name="sender">

		<id-field/>
		<code-field/>
		<description-field/>

	</field-set>

</console-module>
