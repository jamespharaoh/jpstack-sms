<plugin
	name="route-sender"
	package="wbs.sms.route.sender">

	<plugin-dependencies>
		<plugin-dependency name="sms-core"/>
	</plugin-dependencies>

	<models>
		<model name="sender"/>
	</models>

	<layer name="fixture">
		<bean class="fixture.SenderBuilder"/>
		<bean class="fixture.SendersBuilder"/>
	</layer>

	<layer name="model-meta">
		<bean class="metamodel.SenderSpec"/>
		<bean class="metamodel.SendersSpec"/>
	</layer>

	<fixtures>
		<fixture name="sender"/>
	</fixtures>

	<console-modules>
		<console-module name="sender"/>
	</console-modules>

</plugin>
