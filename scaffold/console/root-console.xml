<console-module
	name="root">

	<console-helper-provider
		object-name="root">

		<priv-key
			name="root.manage"
			priv="manage"/>

		<context-stuff
			name="rootName"
			field="code"/>

	</console-helper-provider>

	<object-context
		name="root"
		object-name="root">

		<object>

			<object-summary-page
				form="summary"/>

			<object-events-page/>

		</object>

	</object-context>

	<forms>

		<form
			name="summary"
			object-type="root"
			type="read-only">

			<columns>

				<id-field/>
				<code-field/>

			</columns>

		</form>

	</forms>

</console-module>
