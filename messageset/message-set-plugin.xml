<plugin
	name="message-set"
	package="wbs.sms.messageset">

	<models>
		<model name="messageSet"/>
		<model name="messageSetMessage"/>
		<model name="messageSetType"/>
	</models>

	<layer name="fixture">
		<bean class="fixture.MessageSetTypeBuilder"/>
		<bean class="fixture.MessageSetTypesBuilder"/>
	</layer>

	<layer name="model-meta">
		<bean class="metamodel.MessageSetTypeSpec"/>
		<bean class="metamodel.MessageSetTypesSpec"/>
	</layer>

	<layer name="logic">
		<bean class="logic.MessageSetLogicImpl"/>
	</layer>

	<layer name="console">
		<bean class="console.MessageSetAction"/>
		<bean class="console.MessageSetPart"/>
		<bean class="console.ObjectSmsMessageSetPageBuilder"/>
		<bean class="console.ObjectSmsMessageSetPageSpec"/>
		<bean class="console.SimpleMessageSetFinder"/>
	</layer>

</plugin>
